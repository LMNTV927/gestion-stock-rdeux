generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id           String   @id @default(uuid())
  name         String
  quantity     Int
  unit         String
  min          Int
  unitPrice    Float
  category     String
  supplier     String?
  image        String?
  lastUpdate   String?
  status       String?
  statusColor  String?
}
